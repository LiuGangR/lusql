jar xf apache-solr-common.jar	
jar xf commons-dbcp.jar	
jar xf je.jar		  
jar xf lucene-core.jar     
jar xf apache-solr-solrj.jar	
jar xf commons-httpclient.jar	
jar xf lucene-snowball.jar
